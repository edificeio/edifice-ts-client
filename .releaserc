{
  "plugins": [
    [
      "@semantic-release/commit-analyzer",
      {
        "preset": "angular",
        "releaseRules": [
          {
            "type": "chore",
            "release": "patch"
          },
          {
            "type": "docs",
            "release": "patch"
          },
          {
            "type": "refactor",
            "release": "patch"
          },
          {
            "type": "style",
            "release": "patch"
          }
        ]
      }
    ],
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@semantic-release/github",
      {
        "successComment": false,
        "failTitle": false
      }
    ]
  ],
  "branches": [
    "main",
    {
      "name": "develop",
      "channel": "develop",
      "prerelease": "develop"
    },
    {
      "name": "develop-b2school",
      "channel": "develop-b2school",
      "prerelease": "develop-b2school"
    },
    {
      "name": "develop-pedago",
      "channel": "develop-pedago",
      "prerelease": "develop-pedago"
    },
    {
      "name": "develop-integration",
      "channel": "develop-integration",
      "prerelease": "develop-integration"
    }
  ]
}